<!-- Level1 Entity List-->
<data>
    <!-- There could be only three types of object of data tag;
     Entitiy
     Template
     Group
     
     Other tags will be INVALID in data tag scope
     -->

    <!-- Birdiot Entity -->
    <Entity type="CEntity" name="Birdiot">
        <Component type="CPositionComponent" name"hebele">
            <position>
                <x>74</x>
                <y>240</y>
            </position>
        </Component>
        
        <Component type="CRenderComponent" name="Render">
            <sceneName>MyScene</sceneName>
            <resourceName>bird.png</resourceName>
            <atlasName>android</atlasName>
            <!-- <positionRef>@CPositionComponent.position</positionRef> CAN NOT BE USED YET! -->
            <anchorPoint>
                <x>0.5</x>
                <y>0.5</y>
            </anchorPoint>
        </Component>
        
        <Component type="CAnimationComponent" name="Animation">
            <renderProperty>@CRenderComponent.position.x</renderProperty>
        </Component>
        
        <Component type="CControllerComponent" name="Controller">
        </Component>
        
        <Component type="CBirdComponent" name="Bird">
        </Component>
    </Entity>
    
    <!-- Group Sample -->
    <Group name="Things">
        <ObjectReference name="Birdiot"/>
    </Group>
</data>
<!-- Level1 Entity List-->
<data>
    <!-- There could be only three types of object of data tag;
        Entitiy
        Template
        Group
     
        Other tags will be INVALID in data tag scope
    -->
    <!-- Template Sample -->
    <Template name="Human">
        <Component type="CPositionComponent" name="Position">
            <name>Baris</name>
            <surename>Yilmaz</surename>
        </Component>
    </Template>
    
    <!-- Entity Samples -->
    <Entity type="CEntity" name="skeletonEntity">
        <Component type="CPositionComponent" name"hebele">
            <position>
                <x></x>
                <y></y>
            </position>
        </Component>
        
        <Component type="CRenderComponent" name="Render">
            <sceneName>MyScene</sceneName>
            <resourceName>android.png</resourceName>
            <positionRef>@Position.position</positionRef>
            <atlasName>android</atlasName>
            <color>white</color>
        </Component>
        
        <Component type="CAnimationComponent" name="Animation">
            <renderRef></renderRef>
            <atlasRef></atlasRef>
        </Component>
        
        <Component type="CControllerComponent" name="Controller">
        </Component>
    </Entity>
    
    <Entity type="CEntity" name="TestEntity">
        <Component type="CTestComponent" name="Test">
            <list type="CPoint">
                <_>
                    <x>13</x>
                    <y>76</y>
                </_>
                <_>
                    <x>12</x>
                    <y>34</y>
                </_>
                
            </list>
            
        <Component type="CFSMComponent" name="FSM">
        </Component>
        
    </Entity>
    
    <!-- Group Sample -->
    <Group name="Things">
        <ObjectReference name="Skeleton"/>
        <ObjectReference name="skeletonEntity"/>
        <ObjectReference name="EmptyEntity"/>
    </Group>
</data>